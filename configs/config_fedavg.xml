<fedwork_cfg name="ffwe" num_of_nodes="10" net_port="2342" net_ip="127.0.0.1" eval_criterion="CrossEntropyLoss" num_of_rounds="1">
	<dataset type="MNIST">
		<var name="heterogeneous">False</var>
		<var name="non_iid_level">0.0</var>
		<var name="train_batch_size">128</var>
		<var name="test_batch_size">256</var>
		<var name="num_workers">1</var>
		<var name="save_graph">True</var>
		<var name="enclosed_info">True</var>
	</dataset>

	<localclients learning_rate="0.001" momentum="0.9" weight_decay="1e-7" optimizer="SGD" platform="cpu">10</localclients>

	<method name="FedAvgMethod43" type="FedAvg" platform="cpu">
		<arch type="FeedForwardNet1">
			<var name="ActivationFunction">relu</var>
			<var name="NumberOfHiddenNodes">1024</var>
		</arch>
		<var name="epochs_num">1</var>
		<var name="args">contributors_percent=10</var>
	</method>
	<method name="FedAvgMethod43e" type="FedAvg" platform="cpu">
		<arch type="FeedForwardNet1">
			<var name="ActivationFunction">relu</var>
			<var name="NumberOfHiddenNodes">1024</var>
		</arch>
		<var name="epochs_num">1</var>
		<var name="args">contributors_percent=10</var>
	</method>
	<report save_log="True" log_over_net="192.168.166.143,23456">
	</report>
</fedwork_cfg>
