		<fig style="style=bmh;colors=black,green,blue,brown,black;linestyles=--,:,-.,-;linewidths=1,1,1,1,1;" name="accu" caption=" " x_axis="round" y_axis="EvaluationAccuracy" y_axis_title="Accuracy" x_axis_title="# rounds" methods="SCAFFOLD50p_corrected,FedPollMethod50p_noniid,SCAFFOLD10p_noniid" labels="SCAFFOLD,FedPoll-MaxMin,FedAvg"/>
		<fig style="style=bmh;colors=black,green,blue,brown,black;linestyles=--,:,-.,-;linewidths=1,1,1,1,1;" name="loss" caption=" " x_axis="time" y_axis="EvaluationLoss" y_axis_title="Loss" x_axis_title="Time" methods="SCAFFOLD50p_corrected,FedPollMethod50p_noniid,SCAFFOLD10p_noniid" labels="SCAFFOLD,FedPoll-MaxMin,FedAvg"/>
		<fig name="accuracy" caption="Evaluation Accuracy" x_axis="round" y_axis="EvaluationAccuracy" methods="FedPollMethod3,FedPollMethod6,FedPollMethod1,FedPollMethod5,FedPollMethod2,FedPollMethod4,FedPollMethod7,FedPollMethod8" x_axis_title="Round" y_axis_title="Accuracy %" labels="FedPoll(epsilon=0.01),FedPoll(epsilon=0.005),FedPoll(epsilon=0.001),FedPoll(epsilon=0.0005),FedPoll(epsilon=0.0001),FedPoll(epsilon=0.00001),FedPollMethod7,FedPollMethod8" y_axis_scale = "100" />
		<fig name="losses" caption=" " x_axis="time" y_axis="EvaluationLoss" methods="FedPollMethod3,FedPollMethod6,FedPollMethod1,FedPollMethod5,FedPollMethod2,FedPollMethod4,FedPollMethod7,FedPollMethod8" x_axis_title="Time(s)" y_axis_title="Loss Value" y_axis_scale = "1" labels="FedPoll(epsilon=0.01),FedPoll(epsilon=0.005),FedPoll(epsilon=0.001),FedPoll(epsilon=0.0005),FedPoll(epsilon=0.0001),FedPoll(epsilon=0.00001),FedPollMethod7,FedPollMethod8"/>
	</report>
</fedwork_cfg>
