<fedwork_cfg num_of_nodes="10" net_port="229334" net_ip="127.0.0.1" eval_criterion="CrossEntropyLoss" num_of_rounds="50">
	<dataset type="MNIST">
		<var name="heterogeneous">False</var>
		<var name="non_iid_level">0.5</var>
		<var name="train_batch_size">128</var>
		<var name="test_batch_size">256</var>
		<var name="num_workers">0</var>
		<var name="save_graph">True</var>
		<var name="enclosed_info">True</var>
	</dataset>

	<localclients learning_rate="0.001" momentum="0.9" weight_decay="1e-7" optimizer="SGD">10</localclients>

	<method name="FedPollNDynamicMethod" type="FedPollN" platform="cpu">
		<arch type="FeedForwardNet1">
			<var name="ActivationFunction">relu</var>
			<var name="NumberOfHiddenNodes">1024</var>
		</arch>
		<var name="epochs_num">3</var>
		<var name="args">bits=4,initial_radius=0.1,contributors_percent=100</var>
	</method>
	<method name="FedPollNDynamic1Method" type="FedPollN" platform="cpu">
		<arch type="FeedForwardNet1">
			<var name="ActivationFunction">relu</var>
			<var name="NumberOfHiddenNodes">1024</var>
		</arch>
		<var name="epochs_num">3</var>
		<var name="args">bits=4,initial_radius=0.01,contributors_percent=100</var>
	</method>
	<method name="FedPollNDynamic2Method" type="FedPollN" platform="cpu">
		<arch type="FeedForwardNet1">
			<var name="ActivationFunction">relu</var>
			<var name="NumberOfHiddenNodes">1024</var>
		</arch>
		<var name="epochs_num">3</var>
		<var name="args">bits=4,initial_radius=0.001,contributors_percent=100</var>
	</method>
	<method name="FedAvgMethod" type="FedAvg" platform="cpu">
		<arch type="FeedForwardNet1">
			<var name="ActivationFunction">relu</var>
			<var name="NumberOfHiddenNodes">1024</var>
		</arch>
		<var name="epochs_num">3</var>
	</method>
	<method name="FedMaxMinMethod" type="FedMaxMin" platform="cpu">
		<arch type="FeedForwardNet1">
			<var name="ActivationFunction">relu</var>
			<var name="NumberOfHiddenNodes">1024</var>
		</arch>
		<var name="epochs_num">3</var>
	</method>
	<method name="FedPAQdMethod" type="FedPAQ" platform="cpu">
		<arch type="FeedForwardNet1">
			<var name="ActivationFunction">relu</var>
			<var name="NumberOfHiddenNodes">1024</var>
		</arch>
		<var name="epochs_num">3</var>
		<var name="args">num_levels=128,r_percent=80</var>
	</method>
	<method name="FedPAQ2Method" type="FedPAQ" platform="cpu">
		<arch type="FeedForwardNet1">
			<var name="ActivationFunction">relu</var>
			<var name="NumberOfHiddenNodes">1024</var>
		</arch>
		<var name="epochs_num">3</var>
		<var name="args">num_levels=4,r_percent=80</var>
	</method>
	<report save_log="True" log_over_net="127.0.0.1,23456">
		<fig name="network_usage_srv_rcv_t" caption="" x_axis="round" y_axis="ServerDataRecvBytes" methods="FedPollNDynamicMethod,FedPAQ2Method" x_axis_title="Time(s)"  y_axis_title="Server Data Received (MB)" x_axis_scale = "1" y_axis_scale = "0.000001"/>
		<fig name="accuracy" caption="Evaluation Accuracy" x_axis="round" y_axis="EvaluationAccuracy" methods="FedPollNDynamicMethod,FedPAQ2Method" x_axis_title="Round" y_axis_title="Accuracy %" y_axis_scale = "100" labels="FedPollN(8-bits),FedPAQdMethod(8-bits)"/>
		<fig name="losses" caption="Loss Of Clients" x_axis="time" y_axis="LossOfClient#Client0,LossOfClient#Client1,LossOfClient#Client2,LossOfClient#Client3,LossOfClient#Client4,LossOfClient#Client5,LossOfClient#Client6,LossOfClient#Client7,LossOfClient#Client8,LossOfClient#Client9" methods="FedPollN5Method" x_axis_title="Time(s)" y_axis_title="Loss Value" y_axis_scale = "1"/>
	</report>
</fedwork_cfg>
