<fedwork_cfg num_of_nodes="10" net_port="24538" net_ip="127.0.0.1" eval_criterion="CrossEntropyLoss" num_of_rounds="100">
	<dataset type="CIFAR10">
		<var name="heterogeneous">False</var>
		<var name="non_iid_level">0.1</var>
		<var name="train_batch_size">128</var>
		<var name="test_batch_size">256</var>
		<var name="num_workers">0</var>
		<var name="save_graph">True</var>
		<var name="enclosed_info">True</var>
	</dataset>

	<localclients learning_rate="0.001" momentum="0.9" weight_decay="1e-7" optimizer="SGD" platform="cuda">10</localclients>
	<method name="FedPollr001Method" type="FedPoll" platform="cuda">
		<arch type="ResNet18">
		</arch>
		<var name="epochs_num">3</var>
		<var name="args">no_r=8,initial_radius=0.001,contributors_percent=30</var>
	</method>
	<method name="FedAvgMethod" type="FedAvg" platform="cuda">
		<arch type="ResNet18">
		</arch>
		<var name="epochs_num">3</var>
		<var name="args">contributors_percent=30</var>
	</method>
	<method name="FedAvgQUniformMethod" type="FedAvgQ" platform="cuda">
		<arch type="ResNet18">
		</arch>
		<var name="epochs_num">3</var>
		<var name="args">levels=3,quantization=Uniform,contributors_percent=30</var>
	</method>
	<method name="FedAvgQRandomizedMethod" type="FedAvgQ" platform="cuda">
		<arch type="ResNet18">
		</arch>
		<var name="epochs_num">3</var>
		<var name="args">levels=7,quantization=Randomized,contributors_percent=30</var>
	</method>
	<method name="FedAvgQQSGDMethod" type="FedAvgQ" platform="cuda">
		<arch type="ResNet18">
		</arch>
		<var name="epochs_num">3</var>
		<var name="args">levels=7,quantization=QSGD,contributors_percent=30</var>
	</method>
	<report save_log="True" log_over_net="127.0.0.1,23456">
		<fig name="network_usage_srv_rcv_t" caption="" x_axis="round" y_axis="ServerDataRecvBytes" methods="FedPollNDynamicMethod,FedPAQ2Method" x_axis_title="Time(s)"  y_axis_title="Server Data Received (MB)" x_axis_scale = "1" y_axis_scale = "0.000001"/>
		<fig name="accuracy" caption="Evaluation Accuracy" x_axis="round" y_axis="EvaluationAccuracy" methods="FedPollNDynamicMethod,FedPollNDynamic1Method,FedPollNDynamic2Method,FedPollNDynamic3Method,FedPAQ2Method,FedAvgMethod" x_axis_title="Round" y_axis_title="Accuracy %" y_axis_scale = "100" labels="FedPollN(4-bits),FedPollN(4-bits),FedPollN(4-bits),FedPollN(4-bits),FedPAQdMethod(4-bits),FedAvg"/>
		<fig name="losses" caption="Loss Of Clients" x_axis="time" y_axis="EvaluationLoss" methods="FedPollNDynamicMethod,FedPollNDynamic1Method,FedPollNDynamic2Method,FedPollNDynamic3Method,FedPAQ2Method,FedAvgMethod" x_axis_title="Time(s)" y_axis_title="Loss Value" y_axis_scale = "1"/>
	</report>
</fedwork_cfg>
