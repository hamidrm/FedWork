<fedwork_cfg name="fedpoll_vgg16" num_of_nodes="10" net_port="23335" net_ip="127.0.0.1" eval_criterion="CrossEntropyLoss" num_of_rounds="20">
	<dataset type="CIFAR10">
		<var name="heterogeneous">False</var>
		<var name="non_iid_level">0.1</var>
		<var name="train_batch_size">128</var>
		<var name="test_batch_size">256</var>
		<var name="num_workers">0</var>
		<var name="save_graph">True</var>
		<var name="enclosed_info">True</var>
	</dataset>

	<localclients learning_rate="0.001" momentum="0.9" weight_decay="1e-7" optimizer="SGD" platform="cuda">10</localclients>
	<method name="FedPollMethod1" type="FedPoll" platform="cuda">
		<arch type="VGG16">
			<var name="NumberOfOutputNodes">10</var>
			<var name="initialize_weights">False</var>
		</arch>
		<var name="epochs_num">3</var>
		<var name="args">no_r=8,epsilon=0.001,contributors_percent=30</var>
	</method>
	<method name="FedAvgMethod" type="FedAvg" platform="cuda">
		<arch type="VGG16">
			<var name="NumberOfOutputNodes">10</var>
			<var name="initialize_weights">False</var>
		</arch>
		<var name="epochs_num">3</var>
		<var name="args">contributors_percent=30</var>
	</method>
	<report save_log="True" log_over_net="192.168.166.143,23456">
		<fig name="accu" caption="" x_axis="round" y_axis="EvaluationAccuracy" methods="FedProx3e80p,SCAFFOLD3e80p,FedAvgQSGD3e80p,FedAvg3e80p,FedPoll3e80p,FedPollN3e80p" labels="FedProx,SCAFFOLD,FedAvgQSGD,FedAvg,FedPoll,FedPollN"/>
		<fig name="loss" caption="" x_axis="round" y_axis="EvaluationLoss" methods="FedProx3e80p,SCAFFOLD3e80p,FedAvgQSGD3e80p,FedAvg3e80p,FedPoll3e80p,FedPollN3e80p" labels="FedProx,SCAFFOLD,FedAvgQSGD,FedAvg,FedPoll,FedPollN"/>
	</report>
</fedwork_cfg>
