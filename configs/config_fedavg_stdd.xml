<fedwork_cfg name="fedavg_monitor_stdd" num_of_nodes="10" net_port="22335" net_ip="127.0.0.1" eval_criterion="CrossEntropyLoss" num_of_rounds="100">
	<dataset type="CIFAR10">
		<var name="heterogeneous">False</var>
		<var name="non_iid_level">0.2</var>
		<var name="train_batch_size">128</var>
		<var name="test_batch_size">256</var>
		<var name="num_workers">0</var>
		<var name="save_graph">True</var>
		<var name="enclosed_info">True</var>
	</dataset>

	<localclients learning_rate="0.001" momentum="0" weight_decay="1e-7" optimizer="SGD" platform="cuda">10</localclients>
	<method name="FedAvgMethod4" type="FedAvg" platform="cuda">
		<arch type="ResNet18">
		</arch>
		<var name="epochs_num">3</var>
		<var name="args">contributors_percent=100</var>
	</method>
	<method name="FedAvgMethod_noniid" type="FedAvg" platform="cuda">
		<arch type="ResNet18">
		</arch>
		<var name="epochs_num">3</var>
		<var name="args">contributors_percent=100</var>
	</method>
	<report save_log="True" log_over_net="192.168.166.143,23456">
		<fig name="stdd" caption="" x_axis="round" y_axis="StdD,EvaluationAccuracy" methods="FedAvgMethod4"  x_axis_range="1,-1"/>
	</report>
</fedwork_cfg>
