<fedwork_cfg name="fedpipe_benchmark" num_of_nodes="10" net_port="23334" net_ip="127.0.0.1" eval_criterion="CrossEntropyLoss" num_of_rounds="100">
	<dataset type="CIFAR10">
		<var name="heterogeneous">False</var>
		<var name="non_iid_level">0.5</var>
		<var name="train_batch_size">128</var>
		<var name="test_batch_size">256</var>
		<var name="num_workers">0</var>
		<var name="save_graph">True</var>
		<var name="enclosed_info">True</var>
	</dataset>

	<localclients learning_rate="0.001" momentum="0.9" weight_decay="1e-7" optimizer="SGD" platform="cpu">10</localclients>
	<method name="FedPipe" type="FedPipe" platform="cpu">
		<arch type="ResNet18">
		</arch>
		<var name="epochs_num">3</var>
		<var name="args">beta1=0.9,beta2=0.99,contributors_percent=50,epsilon=1e-8,eta=0.001</var>
	</method>
	<report save_log="True" log_over_net="192.168.166.143,23456">
